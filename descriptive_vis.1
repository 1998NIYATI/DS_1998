plot(e_quakes$long, type= "l") # p: points, l: lines,b: both
plot(e_quakes$stations, xlab = 'X', 
     ylab = 'Y', main = 'E_quakes',
     col = 'green')


# Horizontal bar plot
barplot(e_quakes$depth, main = 'E_quakes',
        ylab = 'A', col= 'blue',horiz = T,axes=T)


#Histogram
hist(e_quakes$long)
hist(e_quakes$mag, 
     main = 'E_quakes',
     xlab = 'A.', col='yellow')

#Single box plot
boxplot(e_quakes$depth,main="Boxplot")

# Multiple box plots
boxplot(e_quakes[,1:5],main='Multiple')

 #margin of the grid(mar), 
#no of rows and columns(mfrow), 
#whether a border is to be included(bty) 
